{
  "name": "starling-api-web-app-starter",
  "version": "0.0.1",
  "description": "Get started with the Starling API using this simple React app.",
  "main": "index.js",
  "engines": {
    "node": ">=7.5.0",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:personal": "better-npm-run dev:personal",
    "dev:sandbox": "better-npm-run dev:sandbox",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --exec \"node -r dotenv/config -r babel-register\" --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "dotenv": "^4.0.0",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "lokijs": "^1.4.3",
    "moment": "^2.18.1",
    "node-sass": "^3.4.2",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "semantic-ui-react": "^0.67.1",
    "starling-developer-sdk": "^0.1.4",
    "style-loader": "^0.13.1",
    "uid-safe": "^2.1.4",
    "url-loader": "^0.5.6",
    "url-search-params": "^0.6.1",
    "webpack": "^1.12.14",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.24.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "karma": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^3.0.1",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.0.0",
    "redbox-react": "^1.2.10",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
